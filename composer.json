{
    "name": "alfred/geo-locator",
    "description": "Geo Locator",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alfred\\GeoLocator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alfred",
            "email": "alfredleo@gmail.com"
        }
    ],
    "require": {
